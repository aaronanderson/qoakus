<qo = 'https://github.com/aaronanderson/qoakus'>
[qo:ContentType] > nt:base, qo:ContentType
  mixin
  - qo:title (string)
    primary mandatory
  + qo:content (qo:ContentType)
    = qo:ContentType
    multiple
